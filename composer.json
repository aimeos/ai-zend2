{
	"name": "aimeos/ai-zend2",
	"description": "Zend Framework 2 adapter for Aimeos web shops and e-commerce solutions",
	"keywords": ["aimeos", "adapter", "zend", "framework", "shop", "e-commerce"],
	"homepage": "https://aimeos.org/",
	"type": "aimeos-extension",
	"license": "LGPL-3.0-or-later",
	"support": {
		"source": "https://github.com/aimeos/ai-zend2",
		"issues": "https://github.com/aimeos/ai-zend2/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"require": {
		"php": ">=5.3.3",
		"aimeos/aimeos-core": "dev-master",
		"zendframework/zend-config": "~2.3",
		"zendframework/zend-log": "~2.3",
		"zendframework/zend-mail": "~2.3",
		"zendframework/zend-mvc": "~2.3",
		"composer/installers": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\MW\\": "lib/custom/src/MW"
		},
		"classmap": [
			"lib/custom/src"
		]
	}
}
